{
  "name": "BlackSky-MD",
  "description": "A highly extensible WhatsApp Multi-Device bot designed for advanced messaging and AI-powered interactions with robust deployment capabilities",
  "repository": "https://github.com/blacksky-algorithms/blacksky-md",
  "logo": "https://raw.githubusercontent.com/blacksky-algorithms/blacksky-md/main/generated-icon.png",
  "keywords": ["whatsapp-bot", "baileys", "nodejs", "bot", "whatsapp-md", "ai-bot", "multi-device"],
  "success_url": "/",
  "stack": "container",
  "env": {
    "PREFIX": {
      "description": "Bot command prefix",
      "value": ".",
      "required": true
    },
    "AUTO_READ_STATUS": {
      "description": "Automatically read contact status (yes/no)",
      "value": "yes",
      "required": false
    },
    "AUTO_DOWNLOAD_STATUS": {
      "description": "Automatically download contact status (yes/no)",
      "value": "no",
      "required": false
    },
    "PM_PERMIT": {
      "description": "Control bot usage in private messages (yes/no)",
      "value": "no",
      "required": false
    },
    "BOT_NAME": {
      "description": "Name for your bot",
      "value": "ğ”¹ğ•ƒğ”¸â„‚ğ•‚ğ•Šğ•‚ğ•-ğ•„ğ”»",
      "required": false
    },
    "OWNER_NAME": {
      "description": "Your name",
      "required": true
    },
    "OWNER_NUMBER": {
      "description": "Your WhatsApp number (without + and with country code)",
      "required": true
    },
    "SESSION_ID": {
      "description": "WhatsApp session ID",
      "required": true
    },
    "NODE_ENV": {
      "description": "Runtime environment",
      "value": "production",
      "required": true
    },
    "LOG_LEVEL": {
      "description": "Logging level (debug/info/warn/error)",
      "value": "info",
      "required": false
    }
  },
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ],
  "addons": [
    {
      "plan": "heroku-postgresql:essential-0"
    }
  ]
}