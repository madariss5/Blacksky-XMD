{
  "name": "𝔹𝕃𝔸ℂ𝕂𝕊𝕂𝕐-𝕄𝔻",
  "description": "WhatsApp Multi-Device Bot using Node.js and Baileys library",
  "repository": "https://github.com/your-username/your-repo",
  "logo": "https://raw.githubusercontent.com/your-repo/assets/main/f9.jpg",
  "keywords": ["node", "whatsapp", "bot", "baileys"],
  "env": {
    "SESSION_ID": {
      "description": "Session ID for WhatsApp connection",
      "required": false
    },
    "OWNER_NAME": {
      "description": "Name of the bot owner",
      "required": true
    },
    "OWNER_NUMBER": {
      "description": "WhatsApp number of the bot owner (with country code)",
      "required": true,
      "value": "1234567890@s.whatsapp.net"
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}
