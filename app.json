{
  "name": "blacksky-md",
  "description": "Advanced WhatsApp Multi-Device bot with AI capabilities",
  "keywords": ["bot", "node", "baileys", "whatsapp", "ai"],
  "website": "https://github.com/blacksky-algorithms/blacksky-md",
  "repository": "https://github.com/blacksky-algorithms/blacksky-md",
  "success_url": "/",
  "stack": "container",
  "env": {
    "PREFIX": {
      "description": "Bot command prefix",
      "value": ".",
      "required": true
    },
    "OWNER_NUMBER": {
      "description": "Your WhatsApp number (international format without +)",
      "required": true
    },
    "OWNER_NAME": {
      "description": "Your name",
      "required": true
    },
    "BOT_NAME": {
      "description": "Name for your bot",
      "value": "BlackSky-MD",
      "required": false
    },
    "SESSION_ID": {
      "description": "WhatsApp session ID",
      "required": true
    },
    "OPENAI_API_KEY": {
      "description": "Your OpenAI API key for AI features",
      "required": true
    },
    "NODE_ENV": {
      "description": "Node environment",
      "value": "production",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "hobby"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql:hobby-dev"
    }
  ]
}